{
  "version": 3,
  "routes": [
    {
      "src": "^(?:/(.*))$",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block"
      },
      "continue": true
    },
    {
      "src": "^/index\\.html$",
      "headers": {
        "Content-Type": "text/html; charset=utf-8",
        "Cache-Control": "no-cache"
      },
      "continue": true
    },
    {
      "src": "^/assets(?:/(.*))$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^(?:/(.*))$",
      "dest": "/",
      "check": true
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    }
  ],
  "framework": {
    "version": "7.1.9"
  },
  "crons": []
}
