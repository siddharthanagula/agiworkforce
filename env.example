# Auth0 Configuration
# Copy this file to .env and replace with your actual Auth0 credentials

# Your Auth0 Domain (found in Auth0 Dashboard > Applications > Your App > Settings)
VITE_AUTH0_DOMAIN=your-domain.auth0.com

# Your Auth0 Client ID (found in Auth0 Dashboard > Applications > Your App > Settings)
VITE_AUTH0_CLIENT_ID=your-client-id

# Your Auth0 Audience (optional, for API access)
VITE_AUTH0_AUDIENCE=your-api-identifier

# Application URL
VITE_APP_URL=https://agiworkforce.com
